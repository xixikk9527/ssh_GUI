{
  "Nmap": [
    { "name": "快速常用端口（探测+版本）", "command": "nmap -T4 -sC -sV <target>" },
    { "name": "全端口TCP SYN扫描", "command": "nmap -sS -p- -T4 <target>" },
    { "name": "全端口TCP + 版本 + OS", "command": "nmap -sS -p- -sV -O --osscan-guess -T4 <target>" },
    { "name": "仅服务版本识别", "command": "nmap -sV <target>" },
    { "name": "激进扫描（包含脚本/版本/OS）", "command": "nmap -A <target>" },
    { "name": "禁用主机发现（直连）", "command": "nmap -Pn <target>" },
    { "name": "UDP常用端口", "command": "nmap -sU --top-ports 200 -T4 <target>" },
    { "name": "全端口UDP", "command": "nmap -sU -p- -T3 <target>" },
    { "name": "TCP连接扫描（适配防火墙）", "command": "nmap -sT -p- -T3 <target>" },
    { "name": "空扫描（绕过某些过滤）", "command": "nmap -sN <target>" },
    { "name": "Xmas扫描", "command": "nmap -sX <target>" },
    { "name": "FIN扫描", "command": "nmap -sF <target>" },
    { "name": "休眠僵尸扫描（隐蔽）", "command": "nmap -sI <zombie> <target>" },
    { "name": "Decoy诱饵扫描", "command": "nmap -D RND:10 <target>" },
    { "name": "分片绕过", "command": "nmap -f <target>" },
    { "name": "指定MTU绕过", "command": "nmap --mtu 8 <target>" },
    { "name": "伪造源端口53", "command": "nmap --source-port 53 <target>" },
    { "name": "伪造源IP（需支持）", "command": "nmap -S <spoof_ip> <target>" },
    { "name": "IPv6扫描", "command": "nmap -6 <ipv6_target>" },
    { "name": "主机发现Ping扫描", "command": "nmap -sn <cidr>" },
    { "name": "列表扫描（不发包）", "command": "nmap -sL <target>" },
    { "name": "指定端口列表", "command": "nmap -p 22,80,443 <target>" },
    { "name": "端口范围1-65535", "command": "nmap -p 1-65535 <target>" },
    { "name": "最小速率与重试优化", "command": "nmap --min-rate 3000 --max-retries 1 <target>" },
    { "name": "并行优化", "command": "nmap --min-parallelism 10 --max-parallelism 100 <target>" },
    { "name": "Timing加速T5", "command": "nmap -T5 <target>" },
    { "name": "默认脚本扫描", "command": "nmap -sC <target>" },
    { "name": "漏洞脚本扫描", "command": "nmap --script vuln <target>" },
    { "name": "安全脚本扫描", "command": "nmap --script safe <target>" },
    { "name": "Banner识别", "command": "nmap -sV --script=banner <target>" },
    { "name": "HTTP标题与头枚举", "command": "nmap --script http-title,http-headers -p80,443 <target>" },
    { "name": "SSL/TLS加密套件", "command": "nmap --script ssl-enum-ciphers -p443 <target>" },
    { "name": "RDP加密信息", "command": "nmap --script rdp-enum-encryption -p3389 <target>" },
    { "name": "SMB枚举", "command": "nmap --script smb-enum-shares,smb-os-discovery -p445 <target>" },
    { "name": "SMB MS17-010检测", "command": "nmap --script smb-vuln-ms17-010 -p445 <target>" },
    { "name": "FTP匿名登录检测", "command": "nmap --script ftp-anon -p21 <target>" },
    { "name": "SSH认证方法", "command": "nmap --script ssh-auth-methods -p22 <target>" },
    { "name": "DNS域名爆破", "command": "nmap --script dns-brute <target>" },
    { "name": "DNS递归检查", "command": "nmap -sU -p53 --script dns-recursion <target>" },
    { "name": "SNMP信息", "command": "nmap --script snmp-info -p161 <target>" },
    { "name": "MySQL信息", "command": "nmap --script mysql-info -p3306 <target>" },
    { "name": "Redis信息", "command": "nmap --script redis-info -p6379 <target>" },
    { "name": "MongoDB枚举", "command": "nmap --script mongodb-databases -p27017 <target>" },
    { "name": "NFS挂载枚举", "command": "nmap --script nfs-showmount -p2049 <target>" },
    { "name": "NTP服务信息", "command": "nmap --script ntp-info -p123 <target>" },
    { "name": "SIP方法枚举", "command": "nmap --script sip-methods -p5060 <target>" },
    { "name": "HTTP暴力示例（路径）", "command": "nmap --script http-brute --script-args http-brute.path=/admin -p80,443 <target>" },
    { "name": "版本识别增强", "command": "nmap -sV --version-all <target>" },
    { "name": "指定DNS服务器", "command": "nmap --dns-servers 8.8.8.8,1.1.1.1 <target>" },
    { "name": "从文件批量扫描", "command": "nmap -iL targets.txt" },
    { "name": "排除主机", "command": "nmap <target> --exclude <host1,host2>" },
    { "name": "输出统一（oA）", "command": "nmap -oA output/scan <target>" },
    { "name": "输出为文本+XML+Grep", "command": "nmap -oN scan.txt -oX scan.xml -oG scan.grep <target>" }
  ]
}
